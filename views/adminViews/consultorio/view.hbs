<div class="row">
  <div class="col s9">
    <table class="table">
      <thead>
        <tr>
          <th scope="col">Id</th>
          <th scope="col">Tipo</th>
          <th scope="col">Precio</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <th scope="row">1</th>
          <td></td>
          <td></td>
        </tr>
    </table>
  </div>
  <div class="col s3">
    <form action="/admin/create" method="POST">
      <h5>Agregar Consultorio</h5>
      <label>No: </label>
      <input name="numero" type="number" min='0' step="1" required>
      <label>Tipo de consultorio</label>
      <select class="browser-default" name="tipoconsultorio" required>
        <option value="" disabled selected>Selecciona una opci√≥n</option>
        <option value="terapia">Terapia</option>
        <option value="medico">Medico</option>
      </select>
      <br>
      <label>Precio: </label><input type="number" min='0' name="precio" required>
      <br><br>
      <label>Color: </label><input type="color" name="color" required>
      <br><br>
      <button class="btn waves-effect waves-light" type="submit" name="action">Submit
        <i class="material-icons right">send</i>
      </button>
    </form>
  </div>
</div>